{
  "manifest_version": 3,
  "name": "Skyharbor Distinguish",
  "version": "1.0",
  "description": "Distinguish between dev, staging, and prod tabs",
  "icons": {
    "16": "images/icon-16.png",
    "32": "images/icon-32.png",
    "48": "images/icon-48.png",
    "128": "images/icon-128.png"
  },
  "action": {
    "default_popup": "popup.html"
  },
  "host_permissions": [
    "https://developer.chrome.com/*",
    "http://*/",
    "https://*/"
  ],
  "permissions": [
    "tabs", "tabGroups", "scripting"
  ],
  "background": {
    "service_worker": "background.js"
  }
}
